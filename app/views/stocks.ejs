    <body>
        <br>
        
        
        <form action="/stocks" method="POST">
            <b>Stock Symbol: </b><input id="stockSymbol" type="text" name="stock_symbol">
            <input type="submit" />
        </form>
        <!-- <button  >Update</button> -->
        <h2>Price</h2>
        <p id="stockPriceUpdate" name="price></p>
        <p id="currentDate"></p>
        
        <h1>Sector Prices</h1>
        <button id="price">Update</button>
        <table id="sectorTable">
            <tr>
                <th>Sector Name</th>
                <th>Real Time</th>
                <th>1 Day</th>
                <th>5 Day</th>
                <th>1 Month</th>
                <th>3 Month</th>
                <th>YTD</th>
                <th>1 Year</th>
                <th>3 Year</th>
                <th>5 Year</th>
                <th>10 Year</th>
            </tr>
            
        </table>
        
        
    </body>
    
